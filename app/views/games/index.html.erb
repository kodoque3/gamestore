<p id="notice"><%= notice %></p>


<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Release year</th>
      <th>Price</th>
      <th>Description</th>
      <th>Coverart url</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @games.each do |game| %>
      <tr>
        <td><%= game.title %></td>
        <td><%= game.release_year %></td>
        <td><%= game.price %></td>
        <td><%= game.description %></td>
        <td><%= game.coverart_url %></td>
        <td><%= link_to 'Show', game %></td>
      
      </tr>
    <% end %>
  </tbody>
</table>

